{
  "name": "gestao-uai-api",
  "version": "1.0.0",
  "description": "API de Gest√£o UAI com Multi-tenancy",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "prisma generate && tsup src/server.ts --out-dir dist",
    "start": "node dist/server.js",
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./prisma/schema.prisma",
    "prisma:seed": "ts-node prisma/seed.ts",   
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.19.2",
    "axios": "^1.13.4",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "node-cron": "^3.0.2",
    "prisma": "^6.19.2",
    "tsup": "^8.5.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.19.130",
    "@types/node-cron": "^3.0.7",
    "@types/uuid": "^9.0.1",
    "dotenv-cli": "^11.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.0.4"
  }
}
